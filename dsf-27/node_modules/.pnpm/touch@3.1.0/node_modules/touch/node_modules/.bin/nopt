#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/nopt@1.0.10/node_modules:/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/nopt@1.0.10/node_modules:/home/luirons/workspace/crs/cdh/cdh-bks-15/cdh-dsf-ultm/dsf-27/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
